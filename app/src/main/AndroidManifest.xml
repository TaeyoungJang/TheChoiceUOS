<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.uos.cs.thechoice" >

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kr.ac.uos.cs.thechoice.SplashActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="kr.ac.uos.cs.thechoice.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="kr.ac.uos.cs.thechoice.CoinActivity"
            android:label="@string/title_activity_coin" >
        </activity>
        <activity
            android:name="kr.ac.uos.cs.thechoice.ConsumerProfileActivity"
            android:label="@string/title_activity_consumer_profile" >
        </activity>
        <activity android:name=".ComsumerWriteContent">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
        <activity
            android:name="kr.ac.uos.cs.thechoice.SellerProfileActivity"
            android:label="@string/title_activity_seller_profile" >
        </activity>
    </application>
   

</manifest>
